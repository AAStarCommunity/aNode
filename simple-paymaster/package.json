{
  "name": "anode-simple-paymaster",
  "version": "0.1.0",
  "description": "aNode Simple Paymaster for Cloudflare Workers - Phase 1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc --noEmit && biome check .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240925.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5",
    "wrangler": "^4.40.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "erc-4337",
    "paymaster",
    "account-abstraction",
    "cloudflare-workers",
    "web3",
    "ethereum"
  ],
  "author": "aNode Team",
  "license": "MIT"
}
